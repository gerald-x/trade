{
    "builds":[
        {
        "src": "trade/wsgi.py",
        "use": "@vercel/python",
        "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        },
        {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
                "distDir": "staticfiles_build"
            }
        }
    ],
    "routes": [
        { 
            "src": "/static/userDashboard/(.*)", 
            "dest": "/userDashboard/static/$1" 
        },
        { 
            "src": "/static/adminDashboard/(.*)", 
            "dest": "/adminDashboard/static/$1" 
        },
        {
            "src": "/(.*)",
            "dest": "trade/wsgi.py"
        }
    ]
}