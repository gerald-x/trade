{
    "builds":[
        {
        "src": "trade/wsgi.py",
        "use": "@vercel/python",
        "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.8"
            }
        },
        {
        "src": "build_files.sh",
        "use": "@vercel/static-build",
        "config": {
            "distDir": "staticfiles_output"
        }
        }
    ],
    "routes": [
        {
          "src": "/userDashboard/static/(.*)",
          "dest": "/static/userDashboard/static/$1"
        },
        {
          "src": "/adminDashboard/static/(.*)",
          "dest": "/static/adminDashboard/static/$1"
        },
        {
          "src": "/(.*)",
          "dest": "trade/wsgi.py"
        }
      ]
}