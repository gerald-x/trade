{
    "builds":[{
        "src": "trade/wsgi.py",
        "use": "@vercel/python",
        "config": {
            "maxLambdaSize": "15mb",
            "runtime": "python3.8"
        }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "trade/wsgi.py"
        }
    ]
}